{
  "Platform": {
    "Environment": "YandexCloud"
  },
  "Application": {
    "Checking": {
      "SubmissionDataPageSize": 100,
      "MinimumSubmissionSimilarityScore": 0,
      "MinimumCodeBlocksSimilarityScore": 0
    }
  },
  "Infrastructure": {
    "Persistence": {
      "Postgres": {
        "Database": "stg_asap-checker",
        "MaximumPoolSize": 9,
        "EnableConnectionProviderLogging": true
      }
    }
  },
  "Sentry": {
    "Environment": "Staging"
  }
}